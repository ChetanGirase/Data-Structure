#include<iostream.h>
#include<conio.h>
void main()
{
clrscr();
int a[50]={9,58,45,10,12,5};
int n=6,i,temp,j;

for(i=0;i<n-1;i++)
{
for(j=0;j<n-1;j++)
{
if(a[j]>a[j+1])
{
temp=a[j];
a[j]=a[j+1];
a[j+1]=temp;
}
}
}
cout<<"Element after sorting:"<<endl;
for(i=0;i<n;i++)
{
cout<<a[i]<<endl;
}
getch();
}
